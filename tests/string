class string {
	char[] c;
	int size;
}

string emptystring() {
	return = new string;
	return.size = 0;
}

string newstring(char c) {
	return = new string;
	return.c = new char[1];
	return.size = 1;
	return.c[0] = c;
}

char digit(int n) {
	if (n == 0)
		return='0';
	else if (n == 1)
		return='1';
	else if (n == 2)
		return='2';
	else if (n == 3)
		return='3';
	else if (n == 4)
		return='4';
	else if (n == 5)
		return='5';
	else if (n == 6)
		return='6';
	else if (n == 7)
		return='7';
	else if (n == 8)
		return='8';
	else if (n == 9)
		return='9';
}

string tostring(int n) {
	return = emptystring();
	if (n == 0)
		return = newstring('0');
	else {
		string pre = emptystring();
		if (n < 0) {
			pre = newstring('-');
			n = -n;
		}
		while(n > 0) {
			return = plus(newstring(digit(n%10)), return);
			n = n/10;
		}
		return = plus(pre, return);
	}
}

string plus(string a, string b) {
	return = new string;
	return.c = new char[a.size+b.size];
	return.size = a.size+b.size;
	for (int i = 0; i < a.size; i++;)
		return.c[i] = a.c[i];
	for (int i = 0; i < b.size; i++;)
		return.c[a.size+i] = b.c[i];
}

void print_string(string a) {
	for (int i = 0; i < a.size; i++;)
		print_char(a.c[i]);
}

void main() {
	print_string(plus(tostring(-1234567890), newstring('\n')));
}
