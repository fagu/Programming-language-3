class string {
	char[] c;
	int size;
}

string emptystring() {
	return = new string;
	return.size = 0;
}

string newstring(char c) {
	return = new string;
	return.c = new char[1];
	return.size = 1;
	return.c[0] = c;
}

string tostring(int n) {
	return = emptystring();
	if (n == 0)
		return = newstring('0');
	else {
		string pre = emptystring();
		if (n < 0) {
			pre = newstring('-');
			n = -n;
		}
		while(n > 0) {
			return = plus(newstring('0'+ n%10), return);
			n = n/10;
		}
		return = plus(pre, return);
	}
}

string plus(string a, string b) {
	return = new string;
	return.c = new char[a.size+b.size];
	return.size = a.size+b.size;
	for (int i = 0; i < a.size; i++;)
		return.c[i] = a.c[i];
	for (int i = 0; i < b.size; i++;)
		return.c[a.size+i] = b.c[i];
}

void print_string(string a) {
	for (int i = 0; i < a.size; i++;)
		print_char(a.c[i]);
}

void main() {
/* hi /* hello */ test */
	print_string(plus(tostring(-1234567890), newstring('\n')));
}
