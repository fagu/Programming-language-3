
class punkt {
	int eintrag;
	punkt next;
	void removestart(int i) {
		for (int k = 0; k < i; k++;)
			this.next = this.next.next;
	}
	punkt at(int i) {
		punkt p = this;
		for (int k = 0; k < i; k++;)
			p = p.next;
		return=p;
	}
	int size() {
		punkt p = this;
		for(return = -1; p; return++;)
			p = p.next;
	}
	void insert(int i) {
		punkt s = this.next;
		punkt n = new punkt;
		n.eintrag = i;
		this.next = n;
		n.next = s;
	}
	void push(int n) {
		punkt p = this;
		while(p.next)
			p = p.next;
		punkt neu = new punkt;
		neu.eintrag = n;
		neu.next = null;
		p.next = neu;
	}
}

void print(punkt p) {
	while(p.next) {
		p = p.next;
		print_int(p.eintrag);
	}
}

void main() {
	punkt list = new punkt;
	list.next = null;
	for (int i = -10; i != 10; i++;)
		list.push(i);
	list.removestart(3);
	list.at(3).insert(20);
	list.at(list.size()).insert(21);
	print(list);
	pr(list.size());
}
