
class punkt {
	int eintrag;
	int hasnext;
	punkt next;
}

void add(punkt p, int n) {
	while(p.hasnext)
		p = p.next;
	punkt neu = new punkt;
	neu.eintrag = n;
	neu.hasnext = 0;
	p.hasnext = 1;
	p.next = neu;
}

void print(punkt p) {
	while(p.hasnext) {
		p = p.next;
		p.eintrag;
	}
}

void main() {
	punkt list = new punkt;
	list.hasnext = 0;
	for (int i = -10; i != 10; i++;)
		add(list, i);
	print(list);
}
